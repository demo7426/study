int main()
{
	XGpioPs_Config* ptXGpioPs_Config = XGpioPs_LookupConfig(GPIO_DEVICE_ID);

	XGpioPs tXGpioPs;
					u32 EffectiveAddr
	Status = XGpioPs_CfgInitialize(&Gpio, ConfigPtr,
					ConfigPtr->BaseAddr);
	if (Status != XST_SUCCESS) {
		return XST_FAILURE;
	}
}

